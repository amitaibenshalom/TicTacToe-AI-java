public static double checkValueDepth(char[] board, char a, char c, double total)
	{
		if (isDraw(board))
			return 0;
		if (isPWon(board, c))
			return 10.0;
		if (isPWon(board, switchMark(c)))
			return -10.0;
		for (int i = 0; i < 9; i++)
			if (board[i] == ' ')
			{
				total += checkValueDepth(addMark(board, i+1, a), switchMark(a), c, 0)/10.0;
				board = addMark(board,i+1,' ');
			}
		return total;
	}
